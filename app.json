{
  "name": "Asuna Smart Music",
  "description": "Open-Source bot to play songs in your Telegram's Group Voice Chat. Powered by PyTgCalls.",
  "keywords": ["music", "voicechat", "telegram"],
  "logo": "https://telegra.ph/file/ad370fb4d879fd29a9489.jpg",
  "repository": "https://github.com/zYxDevs/AsunaSmartMusic",
  "stack": "container",
  "env": {
    "OWNER_ID": {
      "description": "Your telegram ID",
      "required": true
    },
    "OWNER_UNAME": {
      "description": "Your telegram Username",
      "required": true
    },
    "SESSION_NAME": {
      "description": "Pyrogram session string",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "A bot token from @BotFather",
      "required": true
    },
    "BOT_NAME": {
      "description": "Your MusicPlayer Bot Name.",
      "required": false,
      "value": ""
    },
    "BOT_USERNAME": {
      "description": "Your bot username, dont put @",
      "required": true
    },
    "HEROKU_API_KEY": {
      "description": "your heroku apikey",
      "required": true
    },
    "HEROKU_APP_NAME": {
      "description": "Your heroku appname",
      "required": true
    },
    "API_ID": {
      "description": "App ID from my.telegram.org/apps",
      "required": true
    },
    "API_HASH": {
      "description": "App hash from my.telegram.org/apps",
      "required": true
    },
    "SUDO_USERS": {
      "description": "List of user IDs counted as admin everywhere (separated by space).",
      "required": true,
      "value": "1249591948"
    },
    "DURATION_LIMIT": {
      "description": "Max audio duration limit for downloads (minutes).",
      "required": true,
      "value": "15"
    },
    "COMMAND_PREFIXES": {
      "description": "Your bot command prefix",
      "required": false,
      "value": "/ ! ."
    },
    "ASSISTANT_USERNAME": {
      "description": "USERNAME ASSISTANT",
      "required": true
    }
  }
}
